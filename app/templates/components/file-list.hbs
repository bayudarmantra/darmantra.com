<div class="row file-list-row">
  <div class="col-md-4 file-list-cell">
    <a href="#"><i class="fa {{icon}} fa-fw fa-lg"></i> <span>{{file.name}}</span></a>
  </div>
  <div class="col-md-2 file-list-cell">
    <span>{{formattedSize}}</span>
  </div>
  <div class="col-md-3 file-list-cell">
    <span>{{file.filetype}}</span>
  </div>
  <div class="col-md-3 file-list-cell">
    <span>{{formattedDate}}</span>
  </div>
</div>
